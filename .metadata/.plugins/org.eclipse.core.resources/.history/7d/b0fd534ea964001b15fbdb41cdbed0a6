package com.app.model;

public class Lot {

	private int car_id;
	private String make;
	private String model;
	private int year;
	private double cost;
	private String status;
	
	public Lot() {
		
	}
	
	public Lot(int car_id,String make, String model, int year, double cost, String status) {
		super();
		this.car_id = car_id;
		this.make = make;
		this.model = model;
		this.year = year;
		this.cost = cost;
		this.status = status;
	}
}
